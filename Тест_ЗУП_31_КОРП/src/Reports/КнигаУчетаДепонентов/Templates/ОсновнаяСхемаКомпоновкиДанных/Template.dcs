<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Организация</dataPath>
			<field>Организация</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ФизическоеЛицо</dataPath>
			<field>ФизическоеЛицо</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сотрудник</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
			<inputParameters>
				<dcscor:item>
					<dcscor:parameter>СвязиПараметровВыбора</dcscor:parameter>
					<dcscor:value xsi:type="dcscor:ChoiceParameterLinks">
						<dcscor:item>
							<dcscor:choiceParameter>Отбор.Организация</dcscor:choiceParameter>
							<dcscor:value>Организация</dcscor:value>
							<dcscor:mode xmlns:d8p1="http://v8.1c.ru/8.1/data/enterprise" xsi:type="d8p1:LinkedValueChangeMode">Clear</dcscor:mode>
						</dcscor:item>
					</dcscor:value>
				</dcscor:item>
			</inputParameters>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Ведомость</dataPath>
			<field>Ведомость</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Год</dataPath>
			<field>Год</field>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ДФ=гггг</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаВыплаты</dataPath>
			<field>ДатаВыплаты</field>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ДЛФ=D</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаПоступления</dataPath>
			<field>ДатаПоступления</field>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ДЛФ=D</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>МесяцВыплаты</dataPath>
			<field>МесяцВыплаты</field>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ДФ=ММММ</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НомерПлатежногоДокумента</dataPath>
			<field>НомерПлатежногоДокумента</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаВыплаты</dataPath>
			<field>СуммаВыплаты</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаПоступления</dataPath>
			<field>СуммаПоступления</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТабельныйНомер</dataPath>
			<field>ТабельныйНомер</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Таб. номер</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОстатокНач</dataPath>
			<field>ОстатокНач</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Остаток нач</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ДАТАВРЕМЯ(1, 1, 1) КАК Период
ПОМЕСТИТЬ Представления_Периоды
ГДЕ
	"НачалоИнтервала" = &amp;НачалоПериода
	И "ОкончаниеИнтервала" = &amp;КонецПериода
	И "Периодичность" = "ГОД"
	И "ИспользоватьКонецПериода" = ЛОЖЬ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВзаиморасчетыСДепонентами.Период КАК Год,
	ВзаиморасчетыСДепонентами.Организация КАК Организация,
	ВзаиморасчетыСДепонентами.ФизическоеЛицо КАК ФизическоеЛицо,
	ВзаиморасчетыСДепонентами.Ведомость КАК Ведомость,
	ВзаиморасчетыСДепонентами.СуммаНачальныйОстаток КАК ОстатокНач,
	ВзаиморасчетыСДепонентами.СуммаКонечныйОстаток КАК ОстатокКон
ПОМЕСТИТЬ ВТОстатки
ИЗ
	РегистрНакопления.ВзаиморасчетыСДепонентами.ОстаткиИОбороты(&amp;НачалоПериода, &amp;КонецПериода, ГОД, , ) КАК ВзаиморасчетыСДепонентами
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Остатки.Организация КАК Организация,
	Остатки.ФизическоеЛицо КАК ФизическоеЛицо,
	Остатки.Ведомость КАК Ведомость
ПОМЕСТИТЬ ВТПоступленияПрошлыхПериодов
ИЗ
	ВТОстатки КАК Остатки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Периоды.Период КАК Год,
	ПоступленияДепонентов.Организация КАК Организация,
	ПоступленияДепонентов.ФизическоеЛицо КАК ФизическоеЛицо,
	ПоступленияДепонентов.Ведомость КАК Ведомость,
	МАКСИМУМ(Остатки.Год) КАК ГодСОстатком
ПОМЕСТИТЬ ВТГодыСОстатками
ИЗ
	Представления_Периоды КАК Периоды
		ЛЕВОЕ СОЕДИНЕНИЕ ВТПоступленияПрошлыхПериодов КАК ПоступленияДепонентов
		ПО (ИСТИНА)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТОстатки КАК Остатки
		ПО (Остатки.Год &lt;= Периоды.Период)
			И (ПоступленияДепонентов.Организация = Остатки.Организация)
			И (ПоступленияДепонентов.ФизическоеЛицо = Остатки.ФизическоеЛицо)
			И (ПоступленияДепонентов.Ведомость = Остатки.Ведомость)

СГРУППИРОВАТЬ ПО
	Периоды.Период,
	ПоступленияДепонентов.Организация,
	ПоступленияДепонентов.ФизическоеЛицо,
	ПоступленияДепонентов.Ведомость
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ГодыСОстатками.Год КАК Год,
	ГодыСОстатками.Организация КАК Организация,
	ГодыСОстатками.ФизическоеЛицо КАК ФизическоеЛицо,
	ГодыСОстатками.Ведомость КАК Ведомость,
	ВзаиморасчетыСДепонентами.Период КАК ДатаПоступления,
	Остатки.ОстатокНач КАК ОстатокНач,
	СУММА(ВзаиморасчетыСДепонентами.Сумма) КАК СуммаПоступления
ПОМЕСТИТЬ ВТПоступления
ИЗ
	ВТГодыСОстатками КАК ГодыСОстатками
		ЛЕВОЕ СОЕДИНЕНИЕ ВТОстатки КАК Остатки
		ПО (Остатки.Год = ГодыСОстатками.ГодСОстатком)
			И (Остатки.Организация = ГодыСОстатками.Организация)
			И (Остатки.ФизическоеЛицо = ГодыСОстатками.ФизическоеЛицо)
			И (Остатки.Ведомость = ГодыСОстатками.Ведомость)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСДепонентами КАК ВзаиморасчетыСДепонентами
		ПО (ВзаиморасчетыСДепонентами.Организация = ГодыСОстатками.Организация)
			И (ВзаиморасчетыСДепонентами.ФизическоеЛицо = ГодыСОстатками.ФизическоеЛицо)
			И (ВзаиморасчетыСДепонентами.Ведомость = ГодыСОстатками.Ведомость)
			И (ВзаиморасчетыСДепонентами.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход))
ГДЕ
	Остатки.ОстатокНач &gt; 0
	И ГодыСОстатками.ГодСОстатком = ГодыСОстатками.Год

СГРУППИРОВАТЬ ПО
	ГодыСОстатками.Год,
	ГодыСОстатками.Организация,
	ГодыСОстатками.ФизическоеЛицо,
	ГодыСОстатками.Ведомость,
	ВзаиморасчетыСДепонентами.Период,
	Остатки.ОстатокНач

ОБЪЕДИНИТЬ

ВЫБРАТЬ РАЗЛИЧНЫЕ
	НАЧАЛОПЕРИОДА(ВзаиморасчетыСДепонентами.Период, ГОД),
	ВзаиморасчетыСДепонентами.Организация,
	ВзаиморасчетыСДепонентами.ФизическоеЛицо,
	ВзаиморасчетыСДепонентами.Ведомость,
	ВзаиморасчетыСДепонентами.Период,
	СУММА(ВзаиморасчетыСДепонентами.Сумма),
	СУММА(ВзаиморасчетыСДепонентами.Сумма)
ИЗ
	РегистрНакопления.ВзаиморасчетыСДепонентами КАК ВзаиморасчетыСДепонентами
ГДЕ
	ВзаиморасчетыСДепонентами.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ВзаиморасчетыСДепонентами.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)

СГРУППИРОВАТЬ ПО
	ВзаиморасчетыСДепонентами.Организация,
	ВзаиморасчетыСДепонентами.ФизическоеЛицо,
	ВзаиморасчетыСДепонентами.Ведомость,
	ВзаиморасчетыСДепонентами.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Поступления.Организация КАК Организация,
	Поступления.ФизическоеЛицо КАК ФизическоеЛицо,
	Поступления.Ведомость КАК Ведомость,
	МИНИМУМ(НАЧАЛОПЕРИОДА(ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)), МЕСЯЦ)) КАК ПервыйМесяцВыплаты
ПОМЕСТИТЬ ВТПервыеМесяцыВыплатыПредварительно
ИЗ
	ВТПоступления КАК Поступления
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСДепонентами КАК ВзаиморасчетыСДепонентами
		ПО (ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)) &gt;= Поступления.Год)
			И (ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)) &lt;= КОНЕЦПЕРИОДА(Поступления.Год, ГОД))
			И (ЕСТЬNULL(ВзаиморасчетыСДепонентами.ВидДвижения, ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)) = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход))
			И Поступления.Организация = ВзаиморасчетыСДепонентами.Организация
			И Поступления.ФизическоеЛицо = ВзаиморасчетыСДепонентами.ФизическоеЛицо
			И Поступления.Ведомость = ВзаиморасчетыСДепонентами.Ведомость

СГРУППИРОВАТЬ ПО
	Поступления.Организация,
	Поступления.ФизическоеЛицо,
	Поступления.Ведомость
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Поступления.Организация КАК Организация,
	Поступления.ФизическоеЛицо КАК ФизическоеЛицо,
	Поступления.Ведомость КАК Ведомость,
	Поступления.ПервыйМесяцВыплаты КАК ПервыйМесяцВыплаты,
	МИНИМУМ(ВзаиморасчетыСДепонентами.Регистратор) КАК Регистратор
ПОМЕСТИТЬ ВТПервыеМесяцыВыплаты
ИЗ
	ВТПервыеМесяцыВыплатыПредварительно КАК Поступления
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСДепонентами КАК ВзаиморасчетыСДепонентами
		ПО Поступления.Организация = ВзаиморасчетыСДепонентами.Организация
			И Поступления.ФизическоеЛицо = ВзаиморасчетыСДепонентами.ФизическоеЛицо
			И Поступления.Ведомость = ВзаиморасчетыСДепонентами.Ведомость
			И (Поступления.ПервыйМесяцВыплаты = НАЧАЛОПЕРИОДА(ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)), МЕСЯЦ))

СГРУППИРОВАТЬ ПО
	Поступления.Организация,
	Поступления.ФизическоеЛицо,
	Поступления.Ведомость,
	Поступления.ПервыйМесяцВыплаты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Поступления.Год КАК Год,
	Поступления.Организация КАК Организация,
	Поступления.ФизическоеЛицо КАК ФизическоеЛицо,
	Сотрудники.Код КАК ТабельныйНомер,
	Поступления.Ведомость КАК Ведомость,
	Поступления.ДатаПоступления КАК ДатаПоступления,
	Поступления.СуммаПоступления КАК СуммаПоступления,
	Поступления.ОстатокНач КАК ОстатокНач,
	НАЧАЛОПЕРИОДА(ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)), МЕСЯЦ) КАК МесяцВыплаты,
	ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаВыплаты,
	ЕСТЬNULL(ВзаиморасчетыСДепонентами.Сумма, 0) КАК СуммаВыплаты,
	ЕСТЬNULL(ВзаиморасчетыСДепонентами.НомерПлатежногоДокумента, "") КАК НомерПлатежногоДокумента
ИЗ
	ВТПоступления КАК Поступления
		ЛЕВОЕ СОЕДИНЕНИЕ ВТПервыеМесяцыВыплаты КАК ПервыеМесяцыВыплаты
		ПО (ПервыеМесяцыВыплаты.Организация = Поступления.Организация)
			И (ПервыеМесяцыВыплаты.ФизическоеЛицо = Поступления.ФизическоеЛицо)
			И (ПервыеМесяцыВыплаты.Ведомость = Поступления.Ведомость)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ВзаиморасчетыСДепонентами КАК ВзаиморасчетыСДепонентами
		ПО (ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)) &gt;= Поступления.Год)
			И (ЕСТЬNULL(ВзаиморасчетыСДепонентами.Период, ДАТАВРЕМЯ(1, 1, 1)) &lt;= КОНЕЦПЕРИОДА(Поступления.Год, ГОД))
			И (ЕСТЬNULL(ВзаиморасчетыСДепонентами.ВидДвижения, ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)) = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход))
			И Поступления.Организация = ВзаиморасчетыСДепонентами.Организация
			И Поступления.ФизическоеЛицо = ВзаиморасчетыСДепонентами.ФизическоеЛицо
			И Поступления.Ведомость = ВзаиморасчетыСДепонентами.Ведомость
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Сотрудники КАК Сотрудники
		ПО (Сотрудники.ФизическоеЛицо = Поступления.ФизическоеЛицо)
			И (Сотрудники.ГоловнаяОрганизация = Поступления.Организация.ГоловнаяОрганизация)
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ОсновныеСотрудникиФизическихЛиц КАК ОсновныеСотрудникиФизическихЛиц
		ПО (ОсновныеСотрудникиФизическихЛиц.Сотрудник = Сотрудники.Ссылка)
			И (ОсновныеСотрудникиФизическихЛиц.ФизическоеЛицо = Сотрудники.ФизическоеЛицо)
			И (&amp;КонецПериода МЕЖДУ ОсновныеСотрудникиФизическихЛиц.ДатаНачала И ОсновныеСотрудникиФизическихЛиц.ДатаОкончания)</query>
	</dataSet>
	<totalField>
		<dataPath>СуммаВыплаты</dataPath>
		<expression>Сумма(СуммаВыплаты)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаПоступления</dataPath>
		<expression>Сумма(СуммаПоступления)</expression>
	</totalField>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Начало периода</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>&amp;Период.ДатаНачала</expression>
		<availableAsField>false</availableAsField>
	</parameter>
	<parameter>
		<name>КонецПериода</name>
		<title xsi:type="v8:LocalStringType">
