		<field xsi:type="DataSetFieldField">
			<dataPath>СледПериодПредставление</dataPath>
			<field>СледПериодПредставление</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>След период представление</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<field>true</field>
				<condition>true</condition>
