			<dataPath>ЭтапВакансии.Длительность</dataPath>
			<field>ЭтапВакансии.Длительность</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Этап вакансии.Длительность</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЭтапВакансии.Возобновление</dataPath>
