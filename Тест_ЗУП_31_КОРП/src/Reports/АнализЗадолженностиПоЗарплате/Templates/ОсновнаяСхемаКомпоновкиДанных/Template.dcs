<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Организация</dataPath>
			<field>Организация</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Подразделение</dataPath>
			<field>Подразделение</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ФизическоеЛицо</dataPath>
			<field>ФизическоеЛицо</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сотрудник</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НачальныйОстаток</dataPath>
			<field>НачальныйОстаток</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Начальный остаток</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КонечныйОстаток</dataPath>
			<field>КонечныйОстаток</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Конечный остаток</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>ВыделятьОтрицательные</dcscor:parameter>
					<dcscor:value xsi:type="xs:boolean">true</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Приход</dataPath>
			<field>Приход</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Приход</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Расход</dataPath>
			<field>Расход</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Расход</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Месяц</dataPath>
			<field>Месяц</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Месяц</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ДФ='МММ гггг'</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОстатокНаКонецПериодаНеНулевой</dataPath>
			<field>ОстатокНаКонецПериодаНеНулевой</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Остаток на конец периода не нулевой</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЭтоПоследнийМесяцПериода</dataPath>
			<field>ЭтоПоследнийМесяцПериода</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Это последний месяц периода</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ЭтоПервыйМесяцПериода</dataPath>
			<field>ЭтоПервыйМесяцПериода</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Это первый месяц периода</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтатьяРасходов</dataPath>
			<field>СтатьяРасходов</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтатьяФинансирования</dataPath>
			<field>СтатьяФинансирования</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДолгНаКонецПериода</dataPath>
			<field>ДолгНаКонецПериода</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Долг на конец периода</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КВыплатеНаКонецПериода</dataPath>
			<field>КВыплатеНаКонецПериода</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>К выплате на конец периода</v8:content>
				</v8:item>
			</title>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧДЦ=2</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ДАТАВРЕМЯ(1, 1, 1) КАК Период
ПОМЕСТИТЬ Представления_Периоды
ГДЕ
	"НачалоИнтервала" = &amp;НачалоПериода
	И "ОкончаниеИнтервала" = &amp;КонецПериода
	И "Периодичность" = "МЕСЯЦ"
	И "ИспользоватьКонецПериода" = ЛОЖЬ
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЗарплатаКВыплатеОстаткиИОбороты.Организация КАК Организация,
	ЗарплатаКВыплатеОстаткиИОбороты.ФизическоеЛицо КАК ФизическоеЛицо,
	ЗарплатаКВыплатеОстаткиИОбороты.Подразделение КАК Подразделение,
	ЗарплатаКВыплатеОстаткиИОбороты.Период КАК Месяц,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеНачальныйОстаток КАК НачальныйОстаток,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеПриход КАК Приход,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеРасход КАК Расход,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеКонечныйОстаток КАК КонечныйОстаток,
	ЗарплатаКВыплатеОстаткиИОбороты.СтатьяФинансирования,
	ЗарплатаКВыплатеОстаткиИОбороты.СтатьяРасходов
ПОМЕСТИТЬ ВТОстаткиИОбороты
{ВЫБРАТЬ
	Организация.*,
	ФизическоеЛицо.*,
	Подразделение.*,
	Месяц,
	НачальныйОстаток,
	Приход,
	Расход,
	КонечныйОстаток}
ИЗ
	РегистрНакопления.ЗарплатаКВыплате.ОстаткиИОбороты(&amp;НачалоПериода, &amp;КонецПериода, МЕСЯЦ, ДвиженияИГраницыПериода, {(Организация), (ФизическоеЛицо), (Подразделение)}) КАК ЗарплатаКВыплатеОстаткиИОбороты
{ГДЕ
	ЗарплатаКВыплатеОстаткиИОбороты.Организация.*,
	ЗарплатаКВыплатеОстаткиИОбороты.ФизическоеЛицо.*,
	ЗарплатаКВыплатеОстаткиИОбороты.Подразделение.*,
	ЗарплатаКВыплатеОстаткиИОбороты.Период КАК Месяц,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеНачальныйОстаток КАК НачальныйОстаток,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеПриход КАК Приход,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеРасход КАК Расход,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеКонечныйОстаток КАК КонечныйОстаток}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ОстаткиИОбороты.Организация КАК Организация,
	ОстаткиИОбороты.ФизическоеЛицо КАК ФизическоеЛицо,
	ОстаткиИОбороты.Подразделение КАК Подразделение,
	ОстаткиИОбороты.СтатьяФинансирования,
	ОстаткиИОбороты.СтатьяРасходов,
	Месяцы.Период КАК Месяц
ПОМЕСТИТЬ ВТСотрудникиМесяцы
ИЗ
	ВТОстаткиИОбороты КАК ОстаткиИОбороты
		ЛЕВОЕ СОЕДИНЕНИЕ Представления_Периоды КАК Месяцы
		ПО (ИСТИНА)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СотрудникиМесяцы.Организация КАК Организация,
	СотрудникиМесяцы.ФизическоеЛицо КАК ФизическоеЛицо,
	СотрудникиМесяцы.Подразделение КАК Подразделение,
	СотрудникиМесяцы.СтатьяФинансирования,
	СотрудникиМесяцы.СтатьяРасходов,
	СотрудникиМесяцы.Месяц,
	МАКСИМУМ(ОстаткиИОбороты.Месяц) КАК ПоследнийМесяцОстатков
ПОМЕСТИТЬ ВТСотрудникиМесяцыОстатков
ИЗ
	ВТСотрудникиМесяцы КАК СотрудникиМесяцы
		ЛЕВОЕ СОЕДИНЕНИЕ ВТОстаткиИОбороты КАК ОстаткиИОбороты
		ПО СотрудникиМесяцы.Организация = ОстаткиИОбороты.Организация
			И СотрудникиМесяцы.ФизическоеЛицо = ОстаткиИОбороты.ФизическоеЛицо
			И СотрудникиМесяцы.Подразделение = ОстаткиИОбороты.Подразделение
			И СотрудникиМесяцы.СтатьяФинансирования = ОстаткиИОбороты.СтатьяФинансирования
			И СотрудникиМесяцы.СтатьяРасходов = ОстаткиИОбороты.СтатьяРасходов
			И СотрудникиМесяцы.Месяц &gt;= ОстаткиИОбороты.Месяц

СГРУППИРОВАТЬ ПО
	СотрудникиМесяцы.Организация,
	СотрудникиМесяцы.ФизическоеЛицо,
	СотрудникиМесяцы.Подразделение,
	СотрудникиМесяцы.СтатьяФинансирования,
	СотрудникиМесяцы.СтатьяРасходов,
	СотрудникиМесяцы.Месяц
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЗарплатаКВыплатеОстаткиИОбороты.Организация КАК Организация,
	ЗарплатаКВыплатеОстаткиИОбороты.ФизическоеЛицо КАК ФизическоеЛицо,
	ЗарплатаКВыплатеОстаткиИОбороты.Подразделение КАК Подразделение,
	ЗарплатаКВыплатеОстаткиИОбороты.СуммаКВыплатеОстаток КАК ОстатокНаКонецПериода,
	ЗарплатаКВыплатеОстаткиИОбороты.СтатьяФинансирования,
	ЗарплатаКВыплатеОстаткиИОбороты.СтатьяРасходов
ПОМЕСТИТЬ ВТОстаткиНаКонецПериода
ИЗ
	РегистрНакопления.ЗарплатаКВыплате.Остатки(&amp;КонецПериода, {(Организация), (ФизическоеЛицо), (Подразделение)}) КАК ЗарплатаКВыплатеОстаткиИОбороты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СотрудникиМесяцы.Организация КАК Организация,
	СотрудникиМесяцы.ФизическоеЛицо КАК ФизическоеЛицо,
	СотрудникиМесяцы.Подразделение КАК Подразделение,
	СотрудникиМесяцы.Месяц КАК Месяц,
	ЕСТЬNULL(ВЫБОР
			КОГДА СотрудникиМесяцы.Месяц = ЗарплатаКВыплатеОстаткиИОбороты.Месяц
				ТОГДА ЗарплатаКВыплатеОстаткиИОбороты.НачальныйОстаток
			ИНАЧЕ ЗарплатаКВыплатеОстаткиИОбороты.КонечныйОстаток
		КОНЕЦ, 0) КАК НачальныйОстаток,
	ЕСТЬNULL(ВЫБОР
			КОГДА СотрудникиМесяцы.Месяц = ЗарплатаКВыплатеОстаткиИОбороты.Месяц
				ТОГДА ЗарплатаКВыплатеОстаткиИОбороты.Приход
			ИНАЧЕ 0
		КОНЕЦ, 0) КАК Приход,
	ЕСТЬNULL(ВЫБОР
			КОГДА СотрудникиМесяцы.Месяц = ЗарплатаКВыплатеОстаткиИОбороты.Месяц
				ТОГДА ЗарплатаКВыплатеОстаткиИОбороты.Расход
			ИНАЧЕ 0
		КОНЕЦ, 0) КАК Расход,
	ЕСТЬNULL(ЗарплатаКВыплатеОстаткиИОбороты.КонечныйОстаток, 0) КАК КонечныйОстаток,
	ВЫБОР
		КОГДА ОстаткиНаКонецПериода.ОстатокНаКонецПериода &lt;&gt; 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ОстатокНаКонецПериодаНеНулевой,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(&amp;КонецПериода, МЕСЯЦ) = СотрудникиМесяцы.Месяц
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ЭтоПоследнийМесяцПериода,
	ВЫБОР
		КОГДА &amp;НачалоПериода = СотрудникиМесяцы.Месяц
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ЭтоПервыйМесяцПериода,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(&amp;КонецПериода, МЕСЯЦ) &lt;&gt; СотрудникиМесяцы.Месяц
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ОстаткиНаКонецПериода.ОстатокНаКонецПериода &gt; 0
					ТОГДА ОстаткиНаКонецПериода.ОстатокНаКонецПериода
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК КВыплатеНаКонецПериода,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(&amp;КонецПериода, МЕСЯЦ) &lt;&gt; СотрудникиМесяцы.Месяц
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ОстаткиНаКонецПериода.ОстатокНаКонецПериода &lt; 0
					ТОГДА ОстаткиНаКонецПериода.ОстатокНаКонецПериода
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК ДолгНаКонецПериода,
	СотрудникиМесяцы.СтатьяФинансирования КАК СтатьяФинансирования,
	СотрудникиМесяцы.СтатьяРасходов КАК СтатьяРасходов
{ВЫБРАТЬ
	Организация.*,
	ФизическоеЛицо.*,
	Подразделение.*,
	Месяц,
	НачальныйОстаток,
	Приход,
	Расход,
	КонечныйОстаток,
	КВыплатеНаКонецПериода,
	ДолгНаКонецПериода,
	ЭтоПоследнийМесяцПериода,
	ЭтоПервыйМесяцПериода,
	ОстатокНаКонецПериодаНеНулевой,
	СтатьяФинансирования.*,
	СтатьяРасходов.*}
ИЗ
	ВТСотрудникиМесяцыОстатков КАК СотрудникиМесяцы
		ЛЕВОЕ СОЕДИНЕНИЕ ВТОстаткиИОбороты КАК ЗарплатаКВыплатеОстаткиИОбороты
		ПО СотрудникиМесяцы.Организация = ЗарплатаКВыплатеОстаткиИОбороты.Организация
			И СотрудникиМесяцы.ФизическоеЛицо = ЗарплатаКВыплатеОстаткиИОбороты.ФизическоеЛицо
			И СотрудникиМесяцы.Подразделение = ЗарплатаКВыплатеОстаткиИОбороты.Подразделение
			И СотрудникиМесяцы.СтатьяФинансирования = ЗарплатаКВыплатеОстаткиИОбороты.СтатьяФинансирования
			И СотрудникиМесяцы.СтатьяРасходов = ЗарплатаКВыплатеОстаткиИОбороты.СтатьяРасходов
			И СотрудникиМесяцы.ПоследнийМесяцОстатков = ЗарплатаКВыплатеОстаткиИОбороты.Месяц
		{ЛЕВОЕ СОЕДИНЕНИЕ ВТОстаткиНаКонецПериода КАК ОстаткиНаКонецПериода
		ПО СотрудникиМесяцы.Организация = ОстаткиНаКонецПериода.Организация
			И СотрудникиМесяцы.ФизическоеЛицо = ОстаткиНаКонецПериода.ФизическоеЛицо
			И СотрудникиМесяцы.Подразделение = ОстаткиНаКонецПериода.Подразделение
			И СотрудникиМесяцы.СтатьяФинансирования = ОстаткиНаКонецПериода.СтатьяФинансирования
			И СотрудникиМесяцы.СтатьяРасходов = ОстаткиНаКонецПериода.СтатьяРасходов}
{ГДЕ
	СотрудникиМесяцы.Организация.*,
	СотрудникиМесяцы.ФизическоеЛицо.*,
	СотрудникиМесяцы.Подразделение.*,
	СотрудникиМесяцы.Месяц КАК Месяц,
