<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Должность</dataPath>
			<field>Должность</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Должность</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаПриема</dataPath>
			<field>ДатаПриема</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Дата приема</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НазначениеРезерва</dataPath>
			<field>НазначениеРезерва</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Назначение резерва</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПозицияРезерва</dataPath>
			<field>ПозицияРезерва</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Позиция резерва</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтажВКомпании</dataPath>
			<field>СтажВКомпании</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Стаж в компании</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СотрудникНаКлючевойПозиции</dataPath>
			<field>СотрудникНаКлючевойПозиции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сотрудник на ключевой позиции</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВРезервеДо</dataPath>
			<field>ВРезервеДо</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>В резерве до</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КлючеваяПозиция</dataPath>
			<field>КлючеваяПозиция</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Ключевая позиция</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Подразделение</dataPath>
			<field>Подразделение</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Подразделение</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПозицияШтатногоРасписания</dataPath>
			<field>ПозицияШтатногоРасписания</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Сотрудник</dataPath>
			<field>Сотрудник</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сотрудник</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Статус</dataPath>
			<field>Статус</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Статус</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВидРезерва</dataPath>
			<field>ВидРезерва</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ФизическоеЛицо</dataPath>
			<field>ФизическоеЛицо</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаУвольнения</dataPath>
			<field>ДатаУвольнения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Дата увольнения</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВозрастСотрудникаНаКлючевойПозиции</dataPath>
			<field>ВозрастСотрудникаНаКлючевойПозиции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Возраст сотрудника на ключевой позиции</v8:content>
				</v8:item>
			</title>
			<valueType>
				<v8:Type>xs:decimal</v8:Type>
				<v8:NumberQualifiers>
					<v8:Digits>3</v8:Digits>
					<v8:FractionDigits>0</v8:FractionDigits>
					<v8:AllowedSign>Any</v8:AllowedSign>
				</v8:NumberQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Возраст</dataPath>
			<field>Возраст</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Возраст</v8:content>
				</v8:item>
			</title>
			<valueType>
				<v8:Type>xs:decimal</v8:Type>
				<v8:NumberQualifiers>
					<v8:Digits>3</v8:Digits>
					<v8:FractionDigits>0</v8:FractionDigits>
					<v8:AllowedSign>Any</v8:AllowedSign>
				</v8:NumberQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ТипРезервиста</dataPath>
			<field>ТипРезервиста</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Тип резервиста</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВРезервеС</dataPath>
			<field>ВРезервеС</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>В резерве с</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоРезервистовВПозиции</dataPath>
			<field>КоличествоРезервистовВПозиции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество резервистов в позиции</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДоляВПозиции</dataPath>
			<field>ДоляВПозиции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Доля в позиции</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СотрудникНаКлючевойПозиции.ДатаНачалаРаботыНаКлючевойПозиции</dataPath>
			<field>ДатаНачалаРаботыНаКлючевойПозиции</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Дата начала работы на ключевой позиции</v8:content>
				</v8:item>
			</title>
			<valueType>
				<v8:Type>xs:dateTime</v8:Type>
				<v8:DateQualifiers>
					<v8:DateFractions>Date</v8:DateFractions>
				</v8:DateQualifiers>
			</valueType>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ИсторияКадровогоРезерваСрезПоследних.ПозицияРезерва,
	ИсторияКадровогоРезерваСрезПоследних.ФизическоеЛицо,
	ИсторияКадровогоРезерваСрезПоследних.Статус,
	ИсторияКадровогоРезерваСрезПоследних.ВидРезерва,
	ВЫБОР
		КОГДА ИсторияКадровогоРезерваСрезПоследних.Статус = ЗНАЧЕНИЕ(перечисление.состояниясогласования.согласовано)
			ТОГДА ИсторияКадровогоРезерваСрезПоследних.Период
		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	КОНЕЦ КАК ВРезервеС,
	ИсторияКадровогоРезерваСрезПоследних.ДатаОкончания КАК ВРезервеДо
ПОМЕСТИТЬ ВТРезервисты
ИЗ
	РегистрСведений.ИсторияКадровогоРезерва.СрезПоследних(
			&amp;Период,
			ДатаОкончания = ДАТАВРЕМЯ(1, 1, 1)
				ИЛИ ДатаОкончания &gt;= &amp;Период) КАК ИсторияКадровогоРезерваСрезПоследних
ГДЕ
	ИсторияКадровогоРезерваСрезПоследних.Статус В(&amp;СтатусыВключения)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Сотрудники.Ссылка КАК Сотрудник,
	&amp;Период КАК Период
ПОМЕСТИТЬ ВТСотрудники
ИЗ
	Справочник.Сотрудники КАК Сотрудники
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЗНАЧЕНИЕ(Справочник.Сотрудники.ПустаяСсылка) КАК Сотрудник,
	ЗНАЧЕНИЕ(Справочник.ФизическиеЛица.ПустаяСсылка) КАК ФизическоеЛицо,
	ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка) КАК Подразделение,
	ЗНАЧЕНИЕ(Справочник.Должности.ПустаяСсылка) КАК Должность,
	ЗНАЧЕНИЕ(Справочник.ШтатноеРасписание.ПустаяСсылка) КАК ДолжностьПоШтатномуРасписанию,
	ДАТАВРЕМЯ(1, 1, 1) КАК РабочееМестоПериодРегистрации,
	ЗНАЧЕНИЕ(Документ.ПриемНаРаботу.ПустаяСсылка) КАК РабочееМестоРегистратор,
	ДАТАВРЕМЯ(1, 1, 1) КАК ДатаРождения,
	ДАТАВРЕМЯ(1, 1, 1) КАК ДатаПриема,
	ДАТАВРЕМЯ(1, 1, 1) КАК ДатаУвольнения
ПОМЕСТИТЬ Представления_КадровыеДанныеСотрудников
ИЗ
	ВТСотрудники КАК ВТСотрудники
ГДЕ
	"ТолькоРазрешенные" = ИСТИНА
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Представления_КадровыеДанныеСотрудников.ФизическоеЛицо,
	МАКСИМУМ(Представления_КадровыеДанныеСотрудников.ДатаПриема) КАК ДатаПриема
ПОМЕСТИТЬ ВТСвернутыеФЛПоДатеПриема
ИЗ
	Представления_КадровыеДанныеСотрудников КАК Представления_КадровыеДанныеСотрудников

СГРУППИРОВАТЬ ПО
	Представления_КадровыеДанныеСотрудников.ФизическоеЛицо
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТРезервисты.ПозицияРезерва,
	ВТРезервисты.ФизическоеЛицо,
	ВЫБОР
		КОГДА ВТРезервисты.Статус = ЗНАЧЕНИЕ(Перечисление.СостоянияСогласования.Рассматривается)
			ТОГДА "Кандидат"
		КОГДА ВТРезервисты.Статус = ЗНАЧЕНИЕ(Перечисление.СостоянияСогласования.Согласовано)
			ТОГДА "Включен в резерв"
		КОГДА ВТРезервисты.Статус = ЗНАЧЕНИЕ(Перечисление.СостоянияСогласования.ПустаяСсылка)
			ТОГДА "Исключен из резерва"
	КОНЕЦ КАК Статус,
	ВТРезервисты.ВидРезерва,
	ВТРезервисты.ВРезервеС,
	ВТРезервисты.ВРезервеДо,
	ВЫБОР
		КОГДА Представления_КадровыеДанныеСотрудников.Сотрудник ЕСТЬ NULL 
			ТОГДА "внешний"
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(Представления_КадровыеДанныеСотрудников.ДатаПриема, ДАТАВРЕМЯ(1, 1, 1)) = ДАТАВРЕМЯ(1, 1, 1)
					ТОГДА "не принят"
				ИНАЧЕ "сотрудник"
			КОНЕЦ
	КОНЕЦ КАК ТипРезервиста
ПОМЕСТИТЬ ВТРезервистыСотрудники
ИЗ
	ВТРезервисты КАК ВТРезервисты
		ЛЕВОЕ СОЕДИНЕНИЕ ВТСвернутыеФЛПоДатеПриема КАК ВТСвернутыеФЛПоДатеПриема
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ Представления_КадровыеДанныеСотрудников КАК Представления_КадровыеДанныеСотрудников
			ПО ВТСвернутыеФЛПоДатеПриема.ФизическоеЛицо = Представления_КадровыеДанныеСотрудников.ФизическоеЛицо
				И ВТСвернутыеФЛПоДатеПриема.ДатаПриема = Представления_КадровыеДанныеСотрудников.ДатаПриема
		ПО ВТРезервисты.ФизическоеЛицо = ВТСвернутыеФЛПоДатеПриема.ФизическоеЛицо
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КадровыйРезерв.Назначение КАК НазначениеРезерва,
	КадровыйРезерв.Ссылка КАК ПозицияРезерва,
	ВЫБОР
		КОГДА КадровыйРезерв.Назначение = ЗНАЧЕНИЕ(Перечисление.НазначенияКадровогоРезерва.ПозицияШтатногоРасписания)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК КлючеваяПозиция,
	КадровыйРезерв.ПозицияШтатногоРасписания КАК ПозицияШтатногоРасписания,
	ВТРезервистыСотрудники.ВидРезерва КАК ВидРезерва,
	ВТРезервистыСотрудники.ФизическоеЛицо КАК ФизическоеЛицо,
	ВТРезервистыСотрудники.Статус,
	ВТРезервистыСотрудники.ВРезервеС,
	ВТРезервистыСотрудники.ВРезервеДо,
	ВТРезервистыСотрудники.ТипРезервиста
ПОМЕСТИТЬ ВТКадровыйРезерв
ИЗ
	Справочник.КадровыйРезерв КАК КадровыйРезерв
		ЛЕВОЕ СОЕДИНЕНИЕ ВТРезервистыСотрудники КАК ВТРезервистыСотрудники
		ПО КадровыйРезерв.Ссылка = ВТРезервистыСотрудники.ПозицияРезерва
ГДЕ
	НЕ КадровыйРезерв.ВАрхиве
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВТКадровыйРезерв.ПозицияРезерва,
	МАКСИМУМ(Представления_КадровыеДанныеСотрудников.Сотрудник) КАК СотрудникНаКлючевойДолжности
ПОМЕСТИТЬ ВТТопы
ИЗ
	ВТКадровыйРезерв КАК ВТКадровыйРезерв
		ЛЕВОЕ СОЕДИНЕНИЕ Представления_КадровыеДанныеСотрудников КАК Представления_КадровыеДанныеСотрудников
		ПО ВТКадровыйРезерв.ПозицияШтатногоРасписания = Представления_КадровыеДанныеСотрудников.ДолжностьПоШтатномуРасписанию
ГДЕ
	ВТКадровыйРезерв.НазначениеРезерва = ЗНАЧЕНИЕ(Перечисление.НазначенияКадровогоРезерва.ПозицияШтатногоРасписания)

СГРУППИРОВАТЬ ПО
	ВТКадровыйРезерв.ПозицияРезерва
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТКадровыйРезерв.ПозицияРезерва,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ВТКадровыйРезерв.ФизическоеЛицо) КАК КоличествоРезервистовВПозиции
ПОМЕСТИТЬ ВТНаполнениеПозиций
ИЗ
	ВТКадровыйРезерв КАК ВТКадровыйРезерв

СГРУППИРОВАТЬ ПО
	ВТКадровыйРезерв.ПозицияРезерва
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТКадровыйРезерв.НазначениеРезерва,
	ВТКадровыйРезерв.ПозицияРезерва,
	ВТКадровыйРезерв.КлючеваяПозиция,
	ВТКадровыйРезерв.ВидРезерва,
	ВТКадровыйРезерв.ФизическоеЛицо,
	ВЫБОР
		КОГДА Представления_КадровыеДанныеСотрудников.ДатаРождения ЕСТЬ NULL 
			ТОГДА 0
		КОГДА Представления_КадровыеДанныеСотрудников.ДатаРождения = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(РАЗНОСТЬДАТ(Представления_КадровыеДанныеСотрудников.ДатаРождения, &amp;Период, МЕСЯЦ) / 12 - 0.5 КАК ЧИСЛО(3, 0))
	КОНЕЦ КАК Возраст,
	ВТКадровыйРезерв.Статус,
	ВТКадровыйРезерв.ВРезервеС,
	ВТКадровыйРезерв.ВРезервеДо,
	ВТКадровыйРезерв.ПозицияШтатногоРасписания КАК ПозицияШтатногоРасписания,
	ЕСТЬNULL(ВТТопы.СотрудникНаКлючевойДолжности, ЗНАЧЕНИЕ(Справочник.Сотрудники.ПустаяСсылка)) КАК СотрудникНаКлючевойПозиции,
	Представления_ДляТопов.РабочееМестоПериодРегистрации КАК ДатаНачалаРаботыНаКлючевойПозиции,
	ВЫБОР
		КОГДА Представления_ДляТопов.ДатаРождения ЕСТЬ NULL 
