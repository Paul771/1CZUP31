		Запрос.Выполнить();
	КонецЕсли;
	
	// Титульный лист
	Если ОписаниеТаблиц.Свойство("Численность2013") Тогда
		Запрос.Текст =
		"ВЫБРАТЬ
		|	КОЛИЧЕСТВО(ФизЛица.ФизическоеЛицо) КАК ЧисленностьРаботников,
		|	ЕСТЬNULL(СУММА(ВЫБОР
		|				КОГДА ФизЛица.ФизическоеЛицо.Пол = ЗНАЧЕНИЕ(Перечисление.ПолФизическогоЛица.Женский)
		|					ТОГДА 1
		|				ИНАЧЕ 0
		|			КОНЕЦ), 0) КАК ЧисленностьЖенщин,
		|	ЕСТЬNULL(СУММА(ВЫБОР
		|				КОГДА СправкиОбИнвалидности.ФизическоеЛицо ЕСТЬ НЕ NULL 
		|					ТОГДА 1
		|				ИНАЧЕ 0
		|			КОНЕЦ), 0) КАК ЧисленностьИнвалидов
		|ИЗ
		|	ВТФизЛицаСОблагаемойБазой КАК ФизЛица
		|		ЛЕВОЕ СОЕДИНЕНИЕ ВТСправкиОбИнвалидности КАК СправкиОбИнвалидности
		|		ПО ФизЛица.ФизическоеЛицо = СправкиОбИнвалидности.ФизическоеЛицо";
		ОписаниеТаблиц.Численность2013 = Запрос.Выполнить().Выгрузить()
	КонецЕсли;
	
	// Таблицы 3 и 6 
	Если ОписаниеТаблиц.Свойство("Доходы2013") Тогда
		ТекстЗапроса =
		"ВЫБРАТЬ
		|	СведенияОДоходах.Период,
		|	СведенияОДоходах.ФизическоеЛицо,
		|	СведенияОДоходах.Начисление,
		|	СведенияОДоходах.Регистратор,
		|	СУММА(СведенияОДоходах.ОбъектОбложенияВзносамиСтатья7) КАК Начислено,
		|	СУММА(СведенияОДоходах.НеОблагаетсяФСС) КАК НеОблагается,
		|	СУММА(СведенияОДоходах.СуммаПревысившаяПределФСС) КАК СуммаПревысившаяПределФСС,
		|	СУММА(СведенияОДоходах.НеОблагаетсяФСС_НС) КАК НеОблагаетсяНС_ПЗ,
		|	СУММА(СведенияОДоходах.БазаФСС_НС) КАК БазаНС_ПЗ,
