	#Если Не &amp;ТипыВладельцевНастроекПрав = "" #Тогда
		КОНЕЦ
	#КонецЕсли
  #КонецЕсли
	)

#ИначеЕсли Не &amp;ОграничениеДоступаНаУровнеЗаписейВключено #Тогда
